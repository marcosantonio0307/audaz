<div class="label">
	<span><%= @title %></span>
</div>

<div class="container-big">
	<p style="float: left;"><input type='button' value='Voltar' onclick='history.go(-1)' class="btn btn-default" /></p>
	<table class="table table-bordered" >
		<thead>
			<th>Id</th>
			<th>SKU</th>
			<th>Nome</th>
			<th>Quantidade</th>
			<th>Contagem</th>
			<th>Categoria</th>
		</thead>
		<tbody id="table">
			<% @products.each do |product| %>
			<tr>
				<td><%= product.id %></td>
				<td><%= product.sku %></td>
				<td><%= product.name %></td>
				<td><%= product.amount %></td>
				<td></td>
				<td><%= product.category %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>